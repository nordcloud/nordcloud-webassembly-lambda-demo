{
  "name": "nordcloud-webassembly-lambda-demo",
  "version": "1.0.0",
  "bin": {
    "nordcloud-webassembly-lambda-demo": "bin/nordcloud-webassembly-lambda-demo.js"
  },
  "scripts": {
    "test": "jest",
    "build": "npm run build:c && npm run build:cpp && npm run build:as",
    "build:c": "cd wasm && $HOME/wasi-sdk-16.0/bin/clang -o demo-c.wasm demo-c.c",
    "build:cpp": "cd wasm && $HOME/wasi-sdk-16.0/bin/clang++ -o demo-cpp.wasm demo-cpp.cpp",
    "build:as": "cd wasm && asc --exportStart _start -o demo-as.wasm demo-as.ts",
    "deploy:dev": "cdk deploy nordcloud-webassembly-demo-dev",
    "diff:dev": "cdk diff nordcloud-webassembly-demo-dev",
    "destryo:dev": "cdk destroy nordcloud-webassembly-demo-dev"
  },
  "devDependencies": {
    "@types/jest": "^27.5.2",
    "@types/node": "10.17.27",
    "@types/prettier": "2.6.0",
    "assemblyscript": "^0.20.16",
    "aws-cdk": "2.35.0",
    "esbuild": "^0.14.53",
    "jest": "^27.5.1",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.9.1",
    "typescript": "~3.9.7"
  },
  "dependencies": {
    "aws-cdk-lib": "2.35.0",
    "constructs": "^10.0.0",
    "source-map-support": "^0.5.21"
  }
}
